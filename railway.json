{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "RAILPACK",
    "buildCommand": "pip install --upgrade pip && pip install -r requirements.txt",
    "watchPatterns": [
      "**/requirements.txt",
      "**/bot.py"
    ]
  },
  "deploy": {
    "runtime": "UNSPECIFIED",
    "numReplicas": 1,
    "sleepApplication": false
  }
}
